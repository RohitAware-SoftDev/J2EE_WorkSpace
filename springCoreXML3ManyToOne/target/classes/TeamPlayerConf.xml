<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="beans.TeamBean" name="team1">
		<property name="id" value="1"/>
		<property name="name" value="Swapnil" />
		<property name="city" value="Solapur" />
		<property name="players" >
			<list>
				<bean class="beans.PlayerBean" name="player1">
					<property name="id" value="1"/>
					<property name="name" value="M.S.Dhoni" />
					<property name="role" value="Captain" />
					<property name="jercy_No" value="07" />
				</bean>
				<bean class="beans.PlayerBean" name="player2">
					<property name="id" value="2"/>
					<property name="name" value="Rohit_Sharma" />
					<property name="role" value="Captain" />
					<property name="jercy_No" value="45" />
				</bean>
			</list>
		</property>
	</bean>
</beans>